		<Command>
			<Key>SelectByPaging</Key>
			<CommandType>Text</CommandType>
			<Text>
			SELECT
				[CompanyID],
				[BankID],
				[BankCode],
				[BankName],
				[SWIFT],
				[AreaID],
				[Address],
				[Phone],
				[ParentID],
				[Level],
				[IsDetail],
				[FullName],
				[FullParentID],
				[ModifyDTM],
				[Remark],
				[AllowUsed],
				[XRecordCount],
				[XRowNum]
			FROM(
				SELECT 
					[CompanyID],
					[BankID],
					[BankCode],
					[BankName],
					[SWIFT],
					[AreaID],
					[Address],
					[Phone],
					[ParentID],
					[Level],
					[IsDetail],
					[FullName],
					[FullParentID],
					[ModifyDTM],
					[Remark],
					[AllowUsed],
					Count(*) Over() as [XRecordCount],
					Row_Number() Over(Order By [CompanyID],[BankID],[BankCode],[BankName],[SWIFT],[AreaID],[Address],[Phone],[ParentID],[Level],[IsDetail],[FullName],[FullParentID],[ModifyDTM],[Remark],[AllowUsed]) as [XRowNum]
				FROM [Bas_Bank]
				WHERE 1=1 #WHERE#
			) a WHERE [XRowNum] BETWEEN #BETWEEN#
			</Text>
		</Command>
