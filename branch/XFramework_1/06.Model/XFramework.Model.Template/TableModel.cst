        {
            get;
            set;
        }
		<% if (i < SourceTable.Columns.Count - 1) Response.Write("\r\n"); %>	
		<%}%>
        
        #endregion
        
        #region 构造函数
        
        #endregion
        
        #region 重写方法
        
        #endregion
        
        #region 公开方法
        
        #endregion
        
        #region 辅助方法
        
        #endregion
	}
}