<%@ CodeTemplate Language="C#" TargetLanguage="C#"  ResponseEncoding="UTF-8" Description="POCO实体" %>

<%@ Property Name="NameSpace" Type="System.String" Category="Context" Description="命名空间" Optional="True" %>
